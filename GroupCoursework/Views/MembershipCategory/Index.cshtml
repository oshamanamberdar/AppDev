@*Define model*@
@model List<MembershipCategory>
@*Define ViewData*@
@{
    ViewData["Title"] = "List of Membership Category";
}
@*Write code*@


        <table class="table table-hover">
            <thead>
            <tr>
                <th>S.No</th>
                <th>Description</th>
                <th>Total Loans</th>
            </tr>
            
            </thead> 
            <tbody>
                @foreach (var item in Model)
                {
                    <tr>
                        <td>1</td>
                        <td>@Html.DisplayFor(modelItem=> item.MembershipCategoryDescription) </td>
                        <td>@Html.DisplayFor(modelItem=> item.MembershipCategoryTotalLoans) </td>
                        <td>
                            <a class="btn btn-outline-primary"><i class="bi bi-pencil-square"></i>Edit</a> |
                            <a class="btn btn-outline-danger"><i class="bi bi-trash"></i>Delete</a>
                        </td>
                    </tr>
                }

            </tbody>
        </table>

