@*Define model*@
@model List<LoanType>
@*Define ViewData*@
@{
    ViewData["Title"] = "List of Loan Type";
}
@*Write code*@


<table class="table table-hover">
    <thead>
    <tr>
        <th>S.No</th>
        <th>Studio Name</th>
        <th>Loan Duration</th>
    </tr>
            
    </thead> 
    <tbody>
    @foreach (var item in Model)
    {
        <tr>
            <td>1</td>
            <td>@Html.DisplayFor(modelItem=> item.LoanTypes) </td>
            <td>@Html.DisplayFor(modelItem=> item.LoanDurantion) </td>
            <td>
                <a class="btn btn-outline-primary"><i class="bi bi-pencil-square"></i>Edit</a> |
                <a class="btn btn-outline-danger"><i class="bi bi-trash"></i>Delete</a>
            </td>
        </tr>
    }

    </tbody>
</table>